<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!-- Author: Seth T. Graham -->

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0"
	prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.CoreConceptsController">
	<children>
		<TreeView fx:id="treeView" layoutX="-3.0" layoutY="32.0"
			maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
			minWidth="-Infinity" onContextMenuRequested="#mouseClick"
			onMouseClicked="#mouseClick" prefHeight="767.0" prefWidth="250.0" />
		<Label fx:id="label" layoutX="247.0" layoutY="14.0"
			wrapText="true">
			<font>
				<Font size="14.0" />
			</font>
			<padding>
				<Insets left="10.0" />
			</padding>
		</Label>
		<Label layoutX="247.0" layoutY="208.0" text="Code Example:">
			<font>
				<Font size="14.0" />
			</font>
			<padding>
				<Insets left="10.0" />
			</padding>
		</Label>
		<ScrollPane layoutX="247.0" layoutY="228.0"
			prefHeight="572.0" prefWidth="555.0">
			<content>
				<Label fx:id="exampleLabel" wrapText="true">
					<padding>
						<Insets left="30.0" />
					</padding>
					<font>
						<Font size="14.0" />
					</font>
				</Label>
			</content>
		</ScrollPane>
		<Button layoutY="2.0" mnemonicParsing="false"
			onAction="#mainMenuButtonPushed" prefHeight="30.0" prefWidth="250.0"
			text="Main Menu">
			<font>
				<Font size="14.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
